<Window x:Class="Zoologo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <ComboBox x:Name="animales">
                <ComboBoxItem>Elefantes</ComboBoxItem>
                <ComboBoxItem>Chimpances</ComboBoxItem>
                <ComboBoxItem>Jirafas</ComboBoxItem>
            </ComboBox>
            <Button x:Name="Enviar" Click="Enviar_Click_1"> Enviar </Button>

        </StackPanel>
    </Grid>
</Window>
